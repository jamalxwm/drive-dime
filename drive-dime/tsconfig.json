    "include": [
      ".plasmo/index.d.ts",
      "./**/*.ts",
      "./**/*.tsx"
    ],
    "compilerOptions": {
      "target": "esnext",
      "lib": ["ESNext", "WebWorker", "DOM"],
      "paths": {
        "~*": [
          "./src/*"
